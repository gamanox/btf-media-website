<div class="section" id="clients-section">
    <div class="container">
        <div class="row d-flex justify-content-center">
            <img src="/assets/img/clients-title.svg" class="section-title clients-title" alt="">
        </div>
        <div class="row">
            {{#each clients}}
            <div class="col-2 client">
                <a href="{{link}}" target="_blank">
                    <img src="{{logo}}" alt="{{nombre}}">
                </a>
            </div>
            {{/each}}
        </div>
    </div>
</div>